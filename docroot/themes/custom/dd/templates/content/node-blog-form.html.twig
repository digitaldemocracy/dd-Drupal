<div class="blog-node-edit">
<div class="vc-video vc-slider-time player-area">
  <div class="ddModule" id="dd-videos" data-youtubeid="{{ elements.videoid }}" data-videourlpath="{{ elements.s3url }}/videos/"></div>
  <video id="player1" class="video-js vjs-default-skin"
  controls preload="auto" width="685" height="390" style="max-width:none;margin:0 auto;"
  data-videoid="{{ elements.videoid }}" poster="{{ elements.thumbnail }}" >
  </video>
</div>
<div class="vc-slider-time clearfix">    
  <div class="vc-btn-and-time clearfix"> 
    <div class="vc-slider">
      <div id="slider-range"></div>
    </div>
    <div class="vc-buttons">     
      <button id="edit-start-button" name="op" value="start clip" class="btn">start clip</button>
      <button id="edit-stop-button" name="op" value="end clip" class="btn">end clip</button>
      <button id="edit-preview-button" name="op" value="preview" class="btn">preview</button>
    </div>
    <div class="vc-timer">
      <div class="vc-start-time"> 
        <div class="start-label"><span>Start Time</span></div>
        <div class="vc-start-min">
          <input class="time" type="text" id="edit-start-min" name="start_min" value="{{ elements.start_min }}" size="5" maxlength="128" />
          <div class="st-min"><span>Min</span></div>
        </div>
        <div class="vc-start-sec">
          <input class="time" type="text" id="edit-start-sec" name="start_sec" value="{{ elements.start_sec }}" size="5" maxlength="128" />
          <div class="st-sec"><span>Sec</span></div>
        </div>
      </div>
      <div class="vc-end-time">
        <div class="end-label"><span>End Time</span></div>
        <div class="vc-end-min">
          <input class="time" type="text" id="edit-end-min" name="end_min" value="{{ elements.end_min }}" size="5" maxlength="128" />
          <div class="end-min"><span>Min</span></div>
        </div>
        <div class="vc-end-sec">
          <input class="time" type="text" id="edit-end-sec" name="end_sec" value="{{ elements.end_sec }}" size="5" maxlength="128" />
          <div class="end-sec"><span>Sec</span></div>
        </div>     
      </div>
    </div>
  </div>
</div>
<div class="vc-clip-details">
{{ form }}
</div>
</div>
{{ attach_library('dd/dd-blog-edit') }}
