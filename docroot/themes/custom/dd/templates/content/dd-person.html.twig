{#
/**
 * @file dd_bill.html.twig
 * Default theme implementation to present DD Bill data.
 *
 * This template is used when viewing DD Bill pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_dd_bill()
 *
 * @ingroup themeable
 */
#}

{{ attach_library('dd/dd-person') }}

<div{{ attributes.addClass('dd-person') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
