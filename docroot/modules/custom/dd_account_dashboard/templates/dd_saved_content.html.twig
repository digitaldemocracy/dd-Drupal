{#
/**
 * @file dd_saved_content.html.twig
 * Default theme implementation to present DD Saved Content data.
 *
 * This template is used when viewing DD Saved Content pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_dd_saved_content()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('dd_saved_content') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
