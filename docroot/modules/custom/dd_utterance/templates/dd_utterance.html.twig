{#
/**
 * @file dd_utterance.html.twig
 * Default theme implementation to present DD Utterance data.
 *
 * This template is used when viewing DD Utterance pages.
 *
 *
 * Available variables:
 * - content: A list of content items. Use 'content' to print all content, or
 * - attributes: HTML attributes for the container element.
 *
 * @see template_preprocess_dd_utterance()
 *
 * @ingroup themeable
 */
#}
<div{{ attributes.addClass('dd_utterance') }}>
  {% if content %}
    {{- content -}}
  {% endif %}
</div>
